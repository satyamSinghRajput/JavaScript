var student = [
    { name: 'Nishant', age: 25 },
    { name: 'Shailesh', age: 45 },
    { name: 'Abhishek', age: 36 },
    { name: 'Akshay', age: 65 },
    { name: 'Ashish', age: 12 },
    { name: 'Uday', age: 31 },
    { name: 'Mayank', age: 45 }
  ];
  
  var namesStartingWithA = [];
  
  for (var i = 0; i < student.length; i++) {
    var name = student[i].name;
    if (name.startsWith("A")) {
      namesStartingWithA.push(name);
    }
  }
  
  console.log(namesStartingWithA);